-- constraints
SET NAMES utf8mb4;
SET @OLD_UNIQUE_CHECKS=@@UNIQUE_CHECKS, UNIQUE_CHECKS = 0;
SET @OLD_FOREIGN_KEY_CHECKS=@@FOREIGN_KEY_CHECKS, FOREIGN_KEY_CHECKS = 0;
SET @OLD_SQL_MODE=@@SQL_MODE, SQL_MODE = 'TRADITIONAL';

-- making schema, which is like the 'folder' or 'directory'
DROP SCHEMA IF EXISTS health_records;
CREATE SCHEMA health_records;
USE health_records;

-- making table
CREATE TABLE patients (
   ID INTEGER NOT NULL PRIMARY KEY,
   First_Name VARCHAR(45),
   Last_Name VARCHAR(45) NOT NULL,
   DOB DATE,
   Sex VARCHAR(10) NOT NULL,
   Admitted DATE NOT NULL,
   Discharged DATE,
   Physician_Assistant VARCHAR(45),
   Emergency_Contact_Phone VARCHAR(14),
   Insurance VARCHAR(45)
)ENGINE InnoDB DEFAULT CHARSET=utf8mb4;

-- Insert data
USE health_records;
SET AUTOCOMMIT = 0;
